# PostgreSQL Database Configuration
# IMPORTANT: Copy this file to .env and change these default values!

POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=n8n

POSTGRES_NON_ROOT_USER=your_postgres_user
POSTGRES_NON_ROOT_PASSWORD=your_secure_password_here

# n8n Encryption Key
# Generate a secure random key: openssl rand -base64 32
ENCRYPTION_KEY=your_secure_encryption_key_here
